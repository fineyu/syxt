page{
  width: 100%;
  height: 100vh;
  background-color: #f8f8f8;
}

.none{
  display: none;
}
/* 产品信息录入 */

.pro_write{
  width: 95vw;
  height: 280rpx;
  background-color: #fff;
  border-radius: 10rpx;
  margin: auto;
  overflow: hidden;
  box-shadow:4rpx 8rpx 10rpx #eee;
}
.pro_adress{
  width: 90%;
  height: 60rpx;
  line-height: 60rpx;
  border-bottom: 1rpx solid #eee;
  margin:20rpx auto;
  overflow: hidden;  
}
.pro_adress input{
  width: 88%;
  height: 60rpx;
  line-height: 60rpx;
  font-family: "黑体";
  font-size: 28rpx;
  color: #c1c1c1;
  float: left;
  padding-left: 20rpx;
}
.pro_adress picker{
  width: 88%;
  height: 60rpx;
  line-height: 60rpx;
  font-family: "黑体";
  font-size: 28rpx;
  color: #c1c1c1;
  float: left;
  padding-left: 20rpx;
}
.pro_adress image{
  width: 27rpx;
  height: 34rpx;
  margin: 16rpx auto;
  float: left;
}
.pro_name{
  width: 90%;
  height: 60rpx;
  line-height: 60rpx;
  border-bottom: 1rpx solid #eee;
  margin:20rpx auto;
  overflow: hidden;  
}
.pro_name input{
  width: 88%;
  height: 60rpx;
  line-height: 60rpx;
  font-family: "黑体";
  font-size: 28rpx;
  color: #c1c1c1;
  float: left;
  padding-left: 20rpx;
}
.pro_name image{
  width: 27rpx;
  height: 27rpx;
  margin: 16rpx auto;
  float: left;
}
.pro_adress .choose{
  float: right;
  width: 18rpx;
  height: 30rpx;
}
.detail_adress  input{
  width: 90%;
  height: 82rpx;
  margin: auto;
  font-family: "黑体";
  font-size: 28rpx;
  color: #c1c1c1;
  padding-left: 20rpx;
}
.pro_describe{
  width: 95vw;
  height: 250rpx;
  background-color: #fff;
  border-radius: 10rpx;
  margin: 20rpx auto;
  position: relative;
  box-shadow:4rpx 8rpx 10rpx #eee;
}
.pro_describe .icon{
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  top:20rpx;
  left:20rpx;
}
.pro_describe input{
  font-family:"黑体";
  font-size:28rpx;
  color:#c1c1c1;
  padding-top:20rpx;
  width:90%;
  height:100%;
  padding-left: 56rpx;
  border-bottom:2rpx solid #eee;
  margin:auto;
}
.des_top{
  width: 100%;
  height: 225rpx;
}
.pro_describe .phone{
  width: 170rpx;
  height: 160rpx;
  margin: 42rpx 20rpx;
}
.uploading{
  font-family: "黑体";
  font-size: 32rpx;
  color: #c1c1c1;
  display: inline;
  position: relative;
  top: -46rpx;
}
.next{
  width: 95vw;
  height:90rpx;
  border-radius: 50rpx;
  margin: auto;
  margin-top: 35rpx;
  line-height: 90rpx;
  text-align: center;
  font-family: "黑体";
  font-size: 36rpx;
  color: #353535;
  background: -webkit-linear-gradient(left, #f9c852 , #fee870); /* Safari 5.1 - 6.0 */
  background: -o-linear-gradient(right, #f9c852, #fee870); /* Opera 11.1 - 12.0 */
  background: -moz-linear-gradient(right, #f9c852, #fee870); /* Firefox 3.6 - 15 */
  background: linear-gradient(to right, #f9c852 , #fee870); /* 标准的语法 */
}
